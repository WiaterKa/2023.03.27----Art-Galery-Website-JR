n  background-color: black;\n}\n\n/* ARTICLES SECTION */\n\n.article {\n  height: 55rem;\n  background-color: black;\n  display: flex;\n  justify-content: center;\n  align-items: flex-start;\n  padding: 11.5rem 16.5rem;\n}\n\nh1 {\n  font-family: \"Big Shoulders Display\";\n  font-style: normal;\n  font-weight: 900;\n  font-size: 7rem;\n  line-height: 7rem;\n  text-transform: uppercase;\n  color: #ffffff;\n  text-align: left;\n  width: 50%;\n}\n\n.info-box {\n  width: 50%;\n}\n\n.h2-primary {\n  font-family: \"Big Shoulders Display\";\n  font-style: normal;\n  font-weight: 900;\n  font-size: 3.6rem;\n  line-height: 3.6rem;\n  text-transform: uppercase;\n  color: #d5966c;\n  padding-bottom: 2.4rem;\n}\n.p-primary {\n  font-family: \"Outfit\";\n  font-style: normal;\n  font-weight: 300;\n  font-size: 2.2rem;\n  line-height: 3.2rem;\n  color: #ffffff;\n  padding-bottom: 3.2rem;\n}\n\n/* FOOTER */\n\nfooter {\n  height: 24.5rem;\n  background-color: #d5966c;\n  width: 100%;\n  height: auto;\n  display: flex;\n  justify-content: space-around;\n  align-items: flex-start;\n  padding: 8rem 16.5rem;\n}\n\n.p-secondary {\n  font-family: \"Outfit\";\n  font-style: normal;\n  font-weight: 300;\n  font-size: 1.8rem;\n  line-height: 2.8rem;\n  color: #151515;\n  width: 43rem;\n}\n\n.socials-container {\n  display: flex;\n  gap: 2rem;\n}\n\nsvg {\n  color: black;\n}\n\nsvg:hover {\n  color: white;\n}\n\n/* RWD */\n\n@media screen and (max-width: 1100px) {\n  footer {\n    padding: 5.6rem 4rem;\n  }\n\n  .article {\n    padding: 8rem 4rem;\n  }\n}\n\n@media screen and (max-width: 760px) {\n  .p-secondary {\n    width: 30rem;\n  }\n}\n\n@media screen and (max-width: 600px) {\n  .article {\n    flex-direction: column;\n    height: auto;\n    padding: 4.8rem 3.2rem;\n  }\n\n  .info-box {\n    width: 100%;\n  }\n\n  h2 {\n    padding-top: 4.8rem;\n  }\n\n  footer {\n    flex-direction: column;\n    padding: 4.8rem 3.2rem;\n    gap: 3.8rem;\n  }\n\n  .p-secondary {\n    width: 100%;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"1889b43f6fb8778b8b788f2b2673d0c0","cacheData":{"env":{}}}